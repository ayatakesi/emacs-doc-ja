#| " (@code{miscs} @var{misc-size} @var{used-miscs} @var{free-miscs})\n"
#| " (@code{strings} @var{string-size} @var{used-strings} @var{free-strings})\n"
#| " (@code{string-bytes} @var{byte-size} @var{used-bytes})\n"

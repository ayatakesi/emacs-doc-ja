#. type: section
#: original_texis/functions.texi:37
#, no-wrap
msgid "What Is a Function?"
msgstr ""

#. type: cindex
#: original_texis/functions.texi:39
#, no-wrap
msgid "return value"
msgstr ""

#. type: cindex
#: original_texis/functions.texi:40

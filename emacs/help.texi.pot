msgstr ""

#. type: itemx
#: original_texis/help.texi:494
#, no-wrap
msgid "mouse-2"
msgstr ""

#. type: table
#: original_texis/help.texi:496
msgid "Follow a hyperlink that you click on."
msgstr ""


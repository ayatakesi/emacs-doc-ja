#: original_texis/dired.texi:1118
#, no-wrap
msgid "i"
msgstr "i"

#. type: cindex
#: original_texis/dired.texi:1119
#, no-wrap
msgid "inserted subdirectory (Dired)"
msgstr "inserted subdirectory (Dired)"


#. type: opindex
#: original_texis/cmdargs.texi:1210
#, no-wrap
msgid "--title"
msgstr ""

#. type: cindex
#: original_texis/cmdargs.texi:1211
#, no-wrap
msgid "frame title, command-line argument"
msgstr ""

#. type: table
#: original_texis/cmdargs.texi:1213
msgid "Specify @var{title} as the title for the initial Emacs frame."
msgstr ""

#. type: Plain text
#: original_texis/cmdargs.texi:1217
msgid ""
"The @samp{--name} option (@pxref{Resources}) also specifies the title for "
"the initial Emacs frame."
msgstr ""


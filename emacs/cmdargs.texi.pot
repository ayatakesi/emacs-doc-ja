msgid "EMACSPATH"
msgstr ""

#. type: vindex
#: original_texis/cmdargs.texi:568
#, no-wrap
msgid "EMACSPATH@r{, environment variable}"
msgstr ""

#. type: table
#: original_texis/cmdargs.texi:572

#, no-wrap
msgid "saving sessions"
msgstr ""

#. type: cindex
#: original_texis/misc.texi:2721
#, no-wrap
msgid "restore session"
msgstr ""

#. type: cindex
#: original_texis/misc.texi:2722
#, no-wrap
msgid "remember editing session"
msgstr ""

#. type: cindex
#: original_texis/misc.texi:2723
#, no-wrap

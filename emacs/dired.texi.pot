#: original_texis/dired.texi:566
#, no-wrap
msgid "dired-unmark-all-marks"
msgstr ""

#. type: table
#: original_texis/dired.texi:569
msgid ""
"Remove all marks from all the files in this Dired buffer "
"(@code{dired-unmark-all-marks})."
msgstr ""

#. type: item
#: original_texis/dired.texi:570
#, no-wrap
msgid "* ? @var{markchar}"
msgstr ""

#. type: itemx
#: original_texis/dired.texi:571
#, no-wrap
msgid "M-@key{DEL}"
msgstr ""

#. type: kindex
